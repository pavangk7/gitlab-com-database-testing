#### <%= warning(migration) %> Migration: <%= migration.name_formatted %>

* Type: <%= type(migration) %>
* Duration: <%= walltime(migration) %>
* Database size change: <%= total_size_change(migration) %>

% if migration.queries.any?
<%= render_pgss_table(migration.queries) %>

% if create_table_queries.present?
  <%= render_migration_column_ordering_suggestion(create_table_queries) %>
% end
% end

<%= render_migration_histogram(migration) %>
